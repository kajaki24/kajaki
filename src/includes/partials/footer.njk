    }, 60);
    var navlinks = Array.from(document.querySelectorAll('a:not(.no-trans)'));
    var navigate = function navigate(linkEl) {
      transWrap.classList.remove('render');
      transWrap.addEventListener('transitionend', function () {
        if (linkEl.href) {
          window.location = linkEl.href;
        }
      });
    };
    navlinks.forEach(function (link) {
      return link.addEventListener('click', function (ev) {
        ev.preventDefault();
        navigate(ev.target);
      });
    });
